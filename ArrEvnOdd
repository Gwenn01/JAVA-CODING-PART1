public class Main {
	public static void main(String[] args) {
		int[] arr = {2, 8, 1, 3, 4, 6, 7, 9, 5};
		String result = "";
	
			
		for(int i = 0; i < arr.length; i++){
    		    if(arr[i]%2 == 0){
    		        result += arr[i];
    		    }
		}
		char[] c = result.toChar
		
		
		for(int i = 0; i < arr.length; i++){
    		    if(arr[i]%2 != 0){
    		        result += arr[i];
    		    }
		}
	     
		System.out.print(result);
		
	}
}