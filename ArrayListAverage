import java.util.*;
public class Main {
	public static void main(String[] args) {
	    ArrayList<Integer> list = new ArrayList<Integer>();
	    list.add(10);
	    list.add(20);
	    list.add(30);
	    list.add(40);
	    list.add(50);
	   double sum = 0, ave;
	   for(int i = 0; i < list.size(); i++){
	        System.out.print(list.get(i) + " ");
	        sum += list.get(i);
	   }
	   ave = sum / list.size();
	   
	   System.out.println();
	   System.out.print("Average: " + ave);
	}
}